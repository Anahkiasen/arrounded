{
  "name": "anahkiasen/arrounded",
  "description": "A set of self-aware Laravel abstracts that know their way around",
  "authors": [
    {
      "name": "Maxime Fabre",
      "email": "ehtnam6@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Arrounded\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Arrounded\\": "tests"
    }
  },
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
    "illuminate/auth": "^5.0",
    "illuminate/container": "^5.0",
    "illuminate/database": "^5.0",
    "illuminate/mail": "^5.0",
    "illuminate/pagination": "^5.0",
    "illuminate/support": "^5.0",
    "illuminate/view": "^5.0",
    "mockery/mockery": "^0.9.4",
    "phpunit/phpunit": "^4.6"
  },
  "suggest": {
    "codesleeve/laravel-stapler": "Uploads and thumbnails managment",
    "dingo/api": "API creation",
    "anahkiasen/former": "Form builder",
    "cviebrock/eloquent-sluggable": "Slugs handling",
    "league/csv": "Allows setting metadata from CSV files",
    "robclancy/presenter": "Allows presenting of models"
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "0.7-dev"
    }
  }
}
